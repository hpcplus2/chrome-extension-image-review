{
  "name": "Image Review",
  "description" : "审查图片属性",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "审查图片",
    "default_icon": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    }
  },
  "background": {
      "scripts": [
          "js/background.js"
      ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "css": [
        "css/style.css"
      ],
      "run_at": "document_end",
      "js": [
        "js/jquery-3.4.1.min.js",
        "js/content_script.js"
      ]
    }
  ]
}
